<h1>Admin Controller - Load Tutors Data from Google Spreadsheet</h1>

    <% @schedule.each do |r| %>
        <h3><%= r["location"] + r["timeslot"].to_s %></h3>
        <table>
        <% r["sessions"].each do |s| %>
            <% if s["tutor"] %>
                <tr><td>tutor</td>
                <td><%= s["tutor"][0] %></td>
                <td><%= showcolour(s["tutor"][1]) %></td></tr>
            <% end %>
            <% if s["students"] %>
                <% if s["students"][0] %>
                    <tr><td>student</td><td><%= s["students"][0][0] %></td>
                    <td><%= showcolour(s["students"][0][1]) %></td></tr>
                <% end %>
                <% if s["students"][2] %>
                    <tr><td>student</td><td><%= s["students"][1][0] %></td>
                    <td><%= showcolour(s["students"][1][1]) %></td></tr>
                <% end %>
            <% end %>
            <% if s["comment"] %>
                <tr><td>comment</td>
                <td><%= s["comment"] %></td></tr>
            <% end %>
            <tr><td>--------------------------</td></tr>
        <% end %>        
        </table>
    <% end %>
<br>
<h2>Colours</h2>
<table>
    <tr>
        <th>Red</th>
        <th>Green</th>
        <th>Blue</th>
        <th>first encounter</th>
        <th>qty</th>

    </tr>
    
    <% @allColours.each do |key, value| %>
        <tr>
            <% key.each do |c| %>
                <td><%= c %></td>
            <% end %>
            <td><%= value[0].to_s +  value[1].to_s %></td>
            <td><%= value[2].to_s %></td>

        </tr>
    <% end %>
</table>

<br>

<%= link_to 'New Google', new_google_path %>
