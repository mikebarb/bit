<h1>Listing tutors</h1>
<%= link_to "all tutor history", tutors_history_path %><br>
<%= link_to 'New Tutor', new_tutor_path %><br>
<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Gname</th>
      <th>Sname</th>
      <th>Pname</th>
      <th>Initials</th>
      <th>Sex</th>
      <th>Subjects</th>
      <th>Comment</th>
      <th>Status</th>
      <th>Email</th>
      <th>Phone</th>
      <th>First Lesson</th>
      <th>First Aid</th>
      <th>BFL</th>
      <th colspan="5"></th>
    </tr>
  </thead>

  <tbody>
    <% @tutors.each do |tutor| %>
      <tr>
        <td><%= tutor.id %></td>
        <td><%= tutor.gname %></td>
        <td><%= tutor.sname %></td>
        <td><%= tutor.pname %></td>
        <td><%= tutor.initials %></td>
        <td><%= tutor.sex %></td>
        <td><%= tutor.subjects %></td>
        <td><%= tutor.comment %></td>
        <td><%= tutor.status %></td>
        <td><%= tutor.email %></td>
        <td><%= tutor.phone %></td>
        <td><%= tutor.firstlesson %></td>
        <td><%= tutor.firstaid %></td>
        <td><%= tutor.bfl %></td>
        <td><%= link_to 'History', tutor_history_path(tutor) %></td>
        <td><%= link_to 'Changes', tutor_change_path(tutor) %></td>
        <td><%= link_to 'Show', tutor %></td>
        <td><%= link_to 'Edit', edit_tutor_path(tutor) %></td>
        <td><%= link_to 'Destroy', tutor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'New Tutor', new_tutor_path %>
<%= link_to 'Show all Tutors on one page', alltutors_path %>
